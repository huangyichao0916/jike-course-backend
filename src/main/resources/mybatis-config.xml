<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <typeAlias type="com.hyc.pojo.CourseLesson" alias="CourseLesson"/>
        <typeAlias type="com.hyc.pojo.User" alias="User"/>
        <typeAlias type="com.hyc.pojo.RechargeRecord" alias="RechargeRecord"/>
        <typeAlias type="com.hyc.pojo.PurchaseCourseLessonRecord" alias="PurchaseCourseLessonRecord"/>
    </typeAliases>
    <mappers>
        <mapper class="com.hyc.dao.CourseLessonMapper"/>
        <mapper class="com.hyc.dao.UserMapper"/>
        <mapper class="com.hyc.dao.RechargeRecordMapper"/>
        <mapper class="com.hyc.dao.PurchaseCourseLessonRecordsMapper"/>
    </mappers>
</configuration>